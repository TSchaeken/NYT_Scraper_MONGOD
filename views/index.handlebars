<div>

  {{#if header}}
  <div>
    <div>
      <h2>{{heading}}</h2>
      <p>{{text}}</p>
      <a href={{ url }}>{{ heading }}</a>
    </div>
    <ul>
      {{#each notes}} {{>notes}} {{/each}}
    </ul>
  </div>
  {{else}}
  <ul>
    {{#each article}} {{>articles}} {{/each}}
  </ul>
  {{/if}}
</div>